@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":HospitalContactsXpmParser" as HospitalContactsXpmParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> HospitalContactsXpmParser : parseCommand(undo)
activate HospitalContactsXpmParser

create UndoCommand
HospitalContactsXpmParser -> UndoCommand
activate UndoCommand

UndoCommand --> HospitalContactsXpmParser
deactivate UndoCommand

HospitalContactsXpmParser --> LogicManager : u
deactivate HospitalContactsXpmParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoHospitalContactsXpm()
activate Model

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
